@using InvestmentPoint.Admin.Domain.Entites
@model Area
@{
    ViewData["Title"] = "EditArea";
    Layout = "~/Views/Shared/Master/_AdminLayout.cshtml";
}

<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0">Area</h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Area</a></li>
                    <li class="breadcrumb-item active">AddArea</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<form method="post">
    <div class="card-body">
        <div class="form-group col-md-6">
            <label for="exampleInputName1" asp-for="AreaName">Area Name</label>
            <input type="text" asp-for="AreaName" class="form-control" id="exampleInputName1" placeholder="Area Name">
        </div>

    </div>
    <!-- /.card-body -->

    <div class="card-footer">
        <button type="submit" class="btn btn-primary">Submit</button>
    </div>
</form>